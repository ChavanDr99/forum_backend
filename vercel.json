{
    "version": 2,
    "builds": [
      { "src": "Server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/Server.js" },
      { "src": "/uploads/(.*)", "dest": "/uploads/$1" },
      { "src": "/fronted/(.*)", "dest": "/fronted/build/$1" },
      { "src": "/(.*)", "dest": "/Server.js" }
    ]
  }
  